{
    "name": "Butcherman/artisan-dev-commands",
    "description": "A collection of commands to run in the Artisan CLI that will make development troubleshooting a little easier",
    "type": "library",
    "require": {
        "php": "^7.2",
        "illuminate/support": "^5.5|^6.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Butcherman",
            "email": "ronbutcher@att.net"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Butcherman\\ArtisanDevCommands\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Butcherman\\ArtisanDevCommands\\ArtisanDevCommandsServiceProvider"
            ]
        }
    }
}
