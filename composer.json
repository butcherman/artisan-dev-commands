{
    "name": "butcherman/artisan-dev-commands",
    "description": "A collection of commands to run in the Artisan CLI that will make development troubleshooting your Laravel project a little easier",
    "type": "library",
    "keywords": [
        "framework",
        "laravel",
        "artisan command",
        "development",
        "dev"
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "^5.5|^6.0"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "^4.3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Butcherman",
            "email": "ronbutcher@att.net"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Butcherman\\ArtisanDevCommands\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Butcherman\\ArtisanDevCommands\\ArtisanDevCommandsServiceProvider"
            ]
        }
    }
}
